namespace TEAC;
uses TEAC, System;

constructor NamedReferenceExpression(
    start : ^SharedOfToken;
    name : ^String);
inherited(start);
begin
    _name.assign(name);
end;

destructor NamedReferenceExpression();
begin
end;

procedure NamedReferenceExpression.Accept(visitor : ^IExpressionVisitor);
begin
    visitor^.Visit(this);
end;

function NamedReferenceExpression.GetName() : ^String;
begin
    GetName := @_name;
end;
