namespace TEAC;
uses TEAC, System;

constructor LiteralBooleanExpression(
    start : ^SharedOfToken;
    value : boolean);
inherited(start);
begin
    _value := value;
end;

destructor LiteralBooleanExpression();
begin
end;

procedure LiteralBooleanExpression.Accept(visitor : ^IExpressionVisitor);
begin
    visitor^.Visit(this);
end;

function LiteralBooleanExpression.GetValue() : boolean;
begin
    GetValue := _value;
end;
