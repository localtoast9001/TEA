namespace TEAC;
uses TEAC, System;

constructor LiteralDoubleExpression(
    start : ^SharedOfToken;
    value : double);
inherited(start);
begin
    _value := value;
end;

destructor LiteralDoubleExpression();
begin
end;

procedure LiteralDoubleExpression.Accept(visitor : ^IExpressionVisitor);
begin
    visitor^.Visit(this);
end;
