namespace TEAC;
uses TEAC, System;

constructor LiteralIntExpression(
    start : ^SharedOfToken;
    value : integer);
inherited(start);
begin
    _value := value;
end;

destructor LiteralIntExpression();
begin
end;

procedure LiteralIntExpression.Accept(visitor : ^IExpressionVisitor);
begin
    visitor^.Visit(this);
end;

function LiteralIntExpression.GetValue() : integer;
begin
    GetValue := _value;
end;

