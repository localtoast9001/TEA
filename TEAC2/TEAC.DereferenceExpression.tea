namespace TEAC;
uses System, TEAC;

constructor DereferenceExpression(
    start : ^SharedOfToken;
    inner : ^ReferenceExpression);
inherited(start);
begin
    _inner := inner;
end;

destructor DereferenceExpression();
begin
    if _inner <> nil then
        delete _inner;
end;

procedure DereferenceExpression.Accept(visitor : ^IExpressionVisitor);
begin
    visitor^.Visit(this);
end;

function DereferenceExpression.GetInner() : ^ReferenceExpresion;
begin
    GetInner := _inner;
end;

