namespace TEAC;
uses System, TEAC;

constructor VarBlock(start : ^SharedOfToken);
inherited(start);
begin
end;

destructor VarBlock();
begin
end;

function VarBlock.GetVariables() : ^LinkedListOfVariableDeclaration;
begin
    GetVariables := @_variables;
end;

function VarBlock.Add(item : ^VariableDeclaration) : boolean;
begin
    Add := _variables.Add(item);
end;
