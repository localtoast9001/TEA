namespace TEAC;
uses TEAC, System;

constructor LiteralStringExpression(
    start : ^SharedOfToken;
    value : ^String);
inherited(start);
begin
    _value.assign(value);
end;

destructor LiteralStringExpression();
begin
end;

procedure LiteralStringExpression.Accept(visitor : ^IExpressionVisitor);
begin
    visitor^.Visit(this);
end;

function LiteralStringExpression.GetValue() : ^String;
begin
    GetValue := @_value;
end;
