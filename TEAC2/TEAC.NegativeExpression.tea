namespace TEAC;
uses TEAC, System;

constructor NegativeExpression(
    start : ^SharedOfToken; 
    inner : ^Expression);
inherited(start);
begin
    _inner := inner;
end;

destructor NegativeExpression();
begin
    if _inner <> nil then
        delete _inner;
end;

procedure NegativeExpression.Accept(visitor : ^IExpressionVisitor);
begin
    visitor^.Visit(this);
end;

function NegativeExpression.GetInner() : ^Expression;
begin
    GetInner := _inner;
end;
