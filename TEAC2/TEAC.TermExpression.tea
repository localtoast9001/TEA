namespace TEAC;
uses System, TEAC;

constructor TermExpression(
    start : ^SharedOfToken;
    left, right : ^Expression;
    operator : TermOperator);
inherited(start);
begin
    _left := left;
    _right := right;
    _operator := operator;
end;

destructor TermExpression();
begin
    if _left <> nil then
        delete _left;
    if _right <> nil then
        delete _right;
end;

procedure TermExpression.Accept(visitor : ^IExpressionVisitor);
begin
    visitor^.Visit(this);
end;
