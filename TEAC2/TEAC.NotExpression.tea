namespace TEAC;
uses TEAC, System;

constructor NotExpression(
    start : ^SharedOfToken; 
    inner : ^Expression);
inherited(start);
begin
    _inner := inner;
end;

destructor NotExpression();
begin
    if _inner <> nil then
        delete _inner;
end;

procedure NotExpression.Accept(visitor : ^IExpressionVisitor);
begin
    visitor^.Visit(this);
end;
