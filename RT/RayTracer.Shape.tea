namespace RayTracer;
uses RayTracer;

destructor Shape();
begin
end;

{
<summary>
    Initializes a new instance of the Shape class.
</summary>
}
constructor Shape();
begin
    _texture := nil;
end;

function Shape.GetTexture() : ^Texture;
begin
    GetTexture := _texture;
end;

procedure Shape.SetTexture(texture : ^Texture);
begin
    _texture := texture;
end;

