    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/TEAC/bin/Debug/net6.0/TEAC.dll",
            "args": [
                "/ISystem/",
                "/Faobj/Debug/System.Console.asm",
                "/Foobj/Debug/System.Console.o",
                "/home/localtoast/code/TEA/System/System.Console.Tea",
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        }

    ]
}